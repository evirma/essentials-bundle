{
    "name": "evirma/essentials-bundle",
    "type": "symfony-bundle",
    "description": "Evirma EssentialsBundle",
    "keywords": [],
    "homepage": "https://meniam.ru",
    "license": "MIT",
    "authors": [
        {
            "name": "Eugene Myazin",
            "email": "meniam@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-xml": "*",
        "ext-mbstring": "*",
        "ext-curl": "*",
        "ext-memcached": "*",
        "ext-zlib": "*",
        "ext-json": "*",
        "ext-dom": "*",
        "ext-pdo": "*",
        "ext-gd": "*",
        "ext-libxml": "*",
        "ext-iconv": "*",
        "evirma/autotext-bundle": "^3.0",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.0",
        "symfony/framework-bundle": "^5.4|^6.0",
        "symfony/security-bundle": "^5.4|^6.0",
        "symfony/templating": "^5.4|^6.0",
        "symfony/translation": "^5.4|^6.0",
        "symfony/validator": "^5.4|^6.0",
        "symfony/asset": "^5.4|^6.0",
        "symfony/process": "^5.4|^6.0",
        "symfony/form": "^5.4|^6.0",
        "symfony/console": "^5.4|^6.0",
        "symfony/yaml": "^5.4|^6.0",
        "symfony/lock": "^5.4|^6.0",
        "symfony/serializer": "^5.4|^6.0",
        "symfony/maker-bundle": "^1.24",
        "symfony/expression-language": "^5.2|^6.0",
        "symfony/monolog-bundle": "^3.6",
        "symfony/webpack-encore-bundle": "^1.8",
        "jetbrains/phpstorm-attributes": "^1.0",
        "league/commonmark": "^1.6.0",
        "meniam/typograph-bundle": "^2.0",
        "tijsverkoyen/css-to-inline-styles": "^2.2.0",
        "antoligy/dom-string-iterators": "^1.0",
        "html2text/html2text": "^4.2",
        "ramsey/uuid-doctrine": "^1.6",
        "symfony/html-sanitizer": "^6.0",
        "symfony/string": "^6.0"
    },
    "require-dev": {
        "symfony/debug-pack": "^1.0",
        "evirma/code-pack": "^1.0"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": { "Evirma\\Bundle\\EssentialsBundle\\": "src/" }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*|^6.0"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
